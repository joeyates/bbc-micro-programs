   50:
   60MODE 6
   70DIM data% 1000,env%(13),save% 20
   80:
   90total%=0
  100FOR e%=1 TO 291
  110READ byte%:total%=total%+byte%
  120NEXT
  130IF total%<>17386 PRINT"DATA ERR":END
  140RESTORE
  150:
  160FOR e%=1 TO 13:READ env%(e%):NEXT
  170p%=data%+6
  180FOR voice%=0 TO 1
  190PROCpoint(voice%)
  200PRINT"COMP VOX :"voice%+1
  210?p%=254
  220FOR e%=1 TO 13
  230p%?e%=env%(e%)
  240NEXT
  250p%=p%+14
  260REPEAT
  270PROCnext
  280IF byte%<255 PROCnext
  290UNTIL byte%=255
  300NEXT voice%
  310p%=p%-1
  320PROCpoint(2)
  330$save%="SAVE SAMPLE "+STR$~data%+" "+STR$~(p%+1)
  340X%=save%:Y%=save% DIV 256
  350PRINT "SAVING"
  360CALL &FFF7
  370END
  380:
  390DEF PROCpoint(k%)
  400pointer%=p%-data%:loc%=data%+2*k%
  410?loc%=pointer%
  420loc%?1=pointer% DIV 256
  430ENDPROC
  440:
  450DEF PROCnext
  460READ byte%:?p%=byte%:p%=p%+1
  470ENDPROC
