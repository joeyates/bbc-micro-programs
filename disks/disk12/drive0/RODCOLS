    1A=1
    5F=15*.006
   10MODE 2
   20XF=13:YF=14
   30FOR I=1 TO 300 STEP .006
   35 GCOL 0,A
   40PLOT 69,500*(1+SIN(XF*I)),500*(1+SIN(YF*I))
   43PLOT 5,500*(1+SIN((XF-1)*I)),500*(1+SIN((YF-1)*I))
   45GCOL 0,0
   47PLOT 69,500*(1+SIN(XF*(I-F))),500*(1+SIN(YF*(I-F)))
   49PLOT 5,500*(1+SIN((XF-1)*(I-F))),500*(1+SIN((YF-1)*(I-F)))
   50A=A MOD 7+1
   60NEXT I
