 10 maxcol% = 7 20 MODE 2 25 REM S% - Maximum iterations 30 S% = 255 35 REM C%() - Mapping of iterations to colours 40 DIM C%(S%) 50 VDU 28, 16, 31, 19, 0 55 REM j - left offset 60 j = -2.2 63 REM k - top offset 65 k = -1.5 67 REM q - image width and height 70 q = 3.2 80 var = maxcol% / LOG(S%) 90 FOR N% = 1 TO S%100   C%(N%) = maxcol% - INT(LOG(N%) * var)110 NEXT120 VDU 19, 1, 5; 0;130 VDU 19, 2, 4; 0;140 VDU 19, 3, 6; 0;150 VDU 19, 4, 2; 0;160 VDU 19, 5, 3; 0;170 VDU 19, 6, 1; 0;180 :190 Q% = 1024195 M% = 1200 REPEAT207   REM L - tile size in units210   L = q / M%220   H% = Q% / M%230   FOR J% = 0 TO M% - 1240     FOR K% = 0 TO M% - 1245       REM Points whose coordinates are divisible by 2247       REM have already been plotted.250       IF K% MOD 2 OR J% MOD 2 PROCpt260     NEXT265   NEXT270   M% = M% * 2290 UNTIL M% = 256300 PRINT "END"310 END320 :330 DEF FNm(u, v)340   N% = -1343   x = 0346   y = 0350   REPEAT360     y2 = y * y370     x2 = x * x380     y = 2 * x * y + v390     x = x2 - y2 + u400     N% = N% + 1410   UNTIL N% = S% OR x2 + y2 >= 4420   =N%430 :440 DEF PROCpt450   x = j + L * J%460   y = k + L * K%470   GCOL 0, C%(FNm(x, y))480   X% = J% * H%490   Y% = K% * H%500   MOVE X%, Y%510   MOVE X% + H% - 1, Y%520   PLOT 85, X%, Y% + H% - 1530   PLOT 85, X% + H% - 1, Y% + H% - 1540 ENDPROC