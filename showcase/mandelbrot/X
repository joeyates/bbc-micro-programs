 10 maxcol% = 7 20 MODE 2 25 REM S% - Maximum iterations 30 S% = 255 35 REM C%() - Mapping of iterations to colours 40 DIM C%(S%) 50 VDU 28, 16, 31, 19, 0 55 REM j - left offset 60 j = -2.2 63 REM k - top offset 65 k = -1.9 80 var = maxcol% / LOG(S%) 90 FOR N% = 1 TO S%100   C%(N%) = maxcol% - INT(LOG(N%) * var)110 NEXT120 VDU 19, 1, 5; 0;130 VDU 19, 2, 4; 0;140 VDU 19, 3, 6; 0;150 VDU 19, 4, 2; 0;160 VDU 19, 5, 3; 0;170 VDU 19, 6, 1; 0;180 :190 Q% = 1024195 M% = 256220   H% = 4 : REM Q% / M%230   FOR J% = 0 TO M% - 1 STEP 2240     FOR K% = 0 TO M% - 1250       PROCpt260     NEXT265   NEXT300 PRINT "END"310 END320 :330 DEF FNm(u, v)340   N% = -1343   x = 0346   y = 0350   REPEAT360     y2 = y * y370     x2 = x * x380     y = 2 * x * y + v390     x = x2 - y2 + u400     N% = N% + 1410   UNTIL N% = S% OR x2 + y2 >= 4420   =N%430 :440 DEF PROCpt442   REM L - tile size in units446   L = 4 / 256450   x = j + L * J%460   y = k + L * K%470   GCOL 0, C%(FNm(x, y))473   PRINT TAB(0, 1); "x:"475   PRINT TAB(0, 2); J%; "   "477   PRINT TAB(0, 3); "y:"478   PRINT TAB(0, 4); K%; "   "480   X% = J% * 4490   Y% = K% * 4510   PLOT 69, X%, Y%540 ENDPROC